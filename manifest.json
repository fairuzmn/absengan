{
  "name": "Absen GAN",
  "version": "1.0",
  "description": "Force Enable Presensi & Conference ETHOL",
  "manifest_version": 2,
  "content_scripts":[
    {
      "matches":["https://ethol.pens.ac.id/course/open"],
      "js": ["content.js"]
    }
  ],
  "browser_action" : {
    "default_popup" : "popup.html",
    "default_icon": "kai_white.png"
  },
  "permissions": [
    "activeTab",
    "https://ethol.pens.ac.id/course/open"
  ]
}